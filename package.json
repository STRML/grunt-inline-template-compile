{
  "name": "grunt-inline-template-compile",
  "description": "Compile inline JS templates in your build, instead of at runtime. Great for projects that want to avoid using the CSP header 'unsafe-eval'.",
  "version": "0.1.0",
  "homepage": "https://github.com/STRML/grunt-inline-template-compile",
  "author": {
    "name": "Samuel Reed",
    "email": "sam@tixelated.com"
  },
  "keywords": [
    "grunt",
    "underscore",
    "lodash",
    "templates",
    "esprima",
    "unsafe-eval",
    "eval",
    "csp",
    "compile"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/STRML/grunt-inline-template-compile.git"
  },
  "bugs": {
    "url": "https://github.com/STRML/grunt-inline-template-compile/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/STRML/grunt-inline-template-compile/blob/master/LICENSE-MIT"
    }
  ],
  "main": "Gruntfile.js",
  "bin": "bin/grunt-inline-template-compile",
  "engines": {
    "node": ">= 0.6.0"
  },
  "scripts": {
    "test": "grunt"
  },
  "config": {
    "precommit": {
      "lint": "grunt jshint"
    }
  },
  "dependencies": {
    "falafel": "~0.1.3",
    "lodash": "~2.4.1"
  },
  "devDependencies": {
    "grunt": "~0.4.0",
    "grunt-contrib-nodeunit": "~0.1.2",
    "grunt-contrib-jshint": "~0.1.1",
    "grunt-contrib-copy": "~0.4.0",
    "grunt-contrib-clean": "~0.4.0",
    "precommit-hook": "~0.3.9"
  }
}
